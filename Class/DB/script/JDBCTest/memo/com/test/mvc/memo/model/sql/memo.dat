memo.add|INSERT INTO tbl_memo(seq, subject, content, mseq, regdate) VALUES(memo_seq.nextval, ?, ?, ?, sysdate)
memo.list|SELECT m.seq, m.subject, m.content, (SELECT name FROM tbl_member WHERE seq = m.mseq) AS mname, m.regdate FROM tbl_memo m WHERE (SELECT ing FROM tbl_member WHERE seq = m.mseq) = 1 ORDER BY seq DESC
memo.updatepoint|UPDATE tbl_member SET point = point + ? WHERE seq = ?
memo.getMseq|SELECT mseq FROM tbl_memo WHERE seq = ?
memo.del|DELETE FROM tbl_memo WHERE seq = ?
memo.search|SELECT m.seq, m.subject, m.content, (SELECT name FROM tbl_member WHERE seq = m.mseq) AS mname, m.regdate FROM tbl_memo m WHERE content LIKE '%' || ? || '%' AND (SELECT ing FROM tbl_member WHERE seq = m.mseq) = 1 ORDER BY seq DESC